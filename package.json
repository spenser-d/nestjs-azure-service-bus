{
  "name": "@nestjs-azure/service-bus",
  "version": "0.1.0",
  "description": "A NestJS custom transporter for Azure Service Bus with full pub-sub, session, and dead-letter queue support",
  "author": "Spenser Dubin",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spenser-d/nestjs-azure-service-bus.git"
  },
  "bugs": {
    "url": "https://github.com/spenser-d/nestjs-azure-service-bus/issues"
  },
  "homepage": "https://github.com/spenser-d/nestjs-azure-service-bus#readme",
  "keywords": [
    "nestjs",
    "azure",
    "service-bus",
    "microservices",
    "transporter",
    "pub-sub",
    "message-queue"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@azure/service-bus": "^7.9.0"
  },
  "peerDependencies": {
    "@azure/identity": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/microservices": "^10.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.8.0"
  },
  "peerDependenciesMeta": {
    "@azure/identity": {
      "optional": true
    }
  },
  "devDependencies": {
    "@azure/identity": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
